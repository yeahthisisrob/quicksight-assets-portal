{
  "name": "quicksight-assets-portal",
  "version": "1.0.0",
  "description": "A comprehensive web portal for managing and exploring AWS QuickSight assets",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "cd backend && npm start",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeahthisisrob/quicksight-assets-portal.git"
  },
  "keywords": [
    "quicksight",
    "aws",
    "dashboard",
    "analytics",
    "metadata",
    "react",
    "typescript",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeahthisisrob/quicksight-assets-portal/issues"
  },
  "homepage": "https://github.com/yeahthisisrob/quicksight-assets-portal#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}